{
  "name": "gsheet",
  "version": "1.0.0",
  "description": "A lightweight TypeScript library that adds agent workspace capabilities to Google Sheets",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "bin": {
    "gsheet": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./schemas": {
      "import": "./dist/schemas.js",
      "require": "./dist/schemas.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "validate": "bun run typecheck && bun run test && bun run size",
    "size": "bun run build && gzip -c dist/index.js | wc -c",
    "demo": "bun --hot examples/web-demo/server.ts"
  },
  "keywords": [
    "google-sheets",
    "agent",
    "ai",
    "workspace",
    "spreadsheet",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "zod": "^3.22.0"
  },
  "dependencies": {
    "googleapis": "^144.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0",
    "zod": "^3.22.0"
  }
}
